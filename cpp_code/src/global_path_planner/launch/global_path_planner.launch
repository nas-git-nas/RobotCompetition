<launch>

	<include file="$(find rplidar_ros)/launch/rplidar.launch"/>
	
	<include file="$(find hector_slam_launch)/launch/tutorial_headless.launch"/>

	<node pkg="global_path_planner" type="global_path_planner" name="global_path_planner"
	output="screen"/>
	
	<node pkg="global_path_planner" type="lpp_server" name="lpp_server" 
	output="screen"/>
	
	<node pkg="rosserial_python" type="serial_node.py" name="serial_node">
		<param name="port" value="/dev/ttyACM0"/>
	</node>
	
</launch>
